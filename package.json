{
  "name": "sbobinamente",
  "version": "1.0.0",
  "description": "E-commerce di appunti e PDF protetti con pagamenti carta via Stripe",
  "main": "index.html",
  "homepage": "https://elisaiannone.github.io/sbobinamente",
  "scripts": {
    "start": "node server.js",
    "dev": "npx vercel dev",
    "deploy": "gh-pages -d . -b gh-pages -f",
    "deploy:prod": "git push origin main && npm run deploy",
    "server": "node server.js",
    "server:prod": "APP_URL=https://elisaiannone.github.io/sbobinamente node server.js",
    "preview": "python3 -m http.server 8000",
    "deploy:vercel": "npx vercel --prod",
    "supabase:deploy": "supabase functions deploy create-checkout-session",
    "deploy:all": "npm run deploy:vercel && npm run deploy",
    "start:all": "npm run server"
  },
  "keywords": [
    "ecommerce",
    "appunti",
    "pdf"
  ],
  "author": "Elisai Iannone",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/elisaiannone/sbobinamente.git"
  },
  "dependencies": {
    "@stripe/react-stripe-js": "^5.6.0",
    "@stripe/stripe-js": "^8.8.0",
    "@supabase/supabase-js": "^2.39.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "stripe": "^14.10.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "gh-pages": "^6.1.0",
    "nodemon": "^3.0.1",
    "vercel": "^32.0.0"
  },
  "type": "module"
}
